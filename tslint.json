{
  "extends": ["tslint:latest", "tslint-react"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": {
      "severity": "error",
      "options": [
          "debug",
          "info",
          "log",
          "time",
          "timeEnd",
          "trace"
      ]
    }
  }
}
